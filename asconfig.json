{
  "targets": {
    "release": {
      "outFile": "build/assemblyscript.wasm",
      "textFile": "build/assemblyscript.wat",
      "optimizeLevel": 3,
      "shrinkLevel": 0,
      "converge": true,
      "noAssert": true,
      "runtime": "stub",
      "importMemory": true,
      "measure": true,
      "sourceMap": true
    }
  },
  "options": {
    "bindings": false,
    "zeroFilledMemory": true,
    "runPasses": true,
    "stats": false,
    "noExportMemory": true
  }
}
